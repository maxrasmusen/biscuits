<form action="/biscuits/<%= @biscuit.id %>" method="post" enctype="multipart/form-data">
<input type="hidden" name="_method" value="PUT">
	<label for="name">Name:</label>
	<input type="text" name="name" value="<%= @biscuit.name %>">
	<div></div>
	<label for="text">Text:</label>
	<textarea name="text"> <%= @biscuit.text %> </textarea>
	<div></div>
	<label for="img"> Image: </label>
	<input type="file" name="img" value="<%= @biscuit.image_url %>">

	 <label>
        <input type="radio" name="rating" value="1" > 1 
      </label>
      <label>
        <input type="radio" name="rating" value="2" > 2 
      </label>
      <label>
        <input type="radio" name="rating" value="3" > 3
      </label>
      <label>
      	<input type="radio" name="rating" value="4" > 4
      </label>
      <label>
      	<input type="radio" name="rating" value="5" > 5
      </label>
    </div>

	<input type="submit" value="Submit">
</form>